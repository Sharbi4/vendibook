# Example environment variables (DO NOT commit secrets to your repo)
# Copy this file to `.env.local` for local development and fill in real values.

# Frontend (Vite) - exposed to the browser (publishable key only)
VITE_CLERK_PUBLISHABLE_KEY=
VITE_CLERK_FRONTEND_API=

# Backend / server-side - never expose these to the browser
CLERK_SECRET_KEY=
DATABASE_URL=

# Optional dev/test keys
# VITE_API_URL=http://localhost:3000
# VITE_DEV_AUTH_BYPASS=true
